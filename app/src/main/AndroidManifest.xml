<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henallux.khal.smartcity">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_agend_nam"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_agend_nam_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ViewInscription"
            android:label="@string/inscription"
            android:parentActivityName=".activity.MainActivity" />
        <activity android:name=".activity.CategoriesActivity" />
        <activity android:name=".activity.ResultSearchActivity" />
        <activity android:name=".activity.EventActivity"></activity>
        
    </application>

</manifest>